(
    emptyPatch
    {
        type            empty;
        inGroups        1(empty);
        nFaces          12000;
        startFace       11810;
    }
    minY
    {
        type            patch;
        nFaces          40;
        startFace       23810;
    }
    maxY
    {
        type            patch;
        nFaces          40;
        startFace       23850;
    }
    maxX
    {
        type            wall;
        inGroups        1(wall);
        nFaces          150;
        startFace       23890;
    }
    water_to_heater
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          240;
