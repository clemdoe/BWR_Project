FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/water";
    object      alpha.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    emptyPatch
    {
        type            empty;
    }
    minY
    {
        type            fixedValue;
        value           uniform 1;
    }
    maxY
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    maxX
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    water_to_heater
    {
        type            calculated;
        value           uniform 1;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform 1;
    }
}


