FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/water";
    object      T.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    emptyPatch
    {
        type            empty;
    }
    minY
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    maxY
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    maxX
    {
        type            fixedValue;
        value           uniform 300;
    }
    water_to_heater
    {
        type            compressible::turbulentTemperatureTwoPhaseRadCoupledMixed;
        Tnbr            T;
        kappaMethod     phaseSystem;
        region          fluid;
        otherPhase      gas;
        qrNbr           none;
        qr              none;
        value           uniform 1123;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 300;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 300;
    }
}


