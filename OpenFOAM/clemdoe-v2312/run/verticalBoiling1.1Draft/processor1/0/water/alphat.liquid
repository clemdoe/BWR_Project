FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/water";
    object      alphat.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 1e-08;

boundaryField
{
    emptyPatch
    {
        type            empty;
    }
    minY
    {
        type            zeroGradient;
    }
    maxY
    {
        type            calculated;
        value           nonuniform List<scalar> 0();
    }
    maxX
    {
        type            compressible::alphatPhaseChangeJayatillekeWallFunction;
        Prt             0.85;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        dmdt            uniform 0;
        value           uniform 1e-08;
    }
    water_to_heater
    {
        type            compressible::alphatWallBoilingWallFunction;
        otherPhase      gas;
        phaseType       liquid;
        Prt             0.85;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        relax           constant 1;
        dmdt            uniform 0;
        partitioningModel
        {
            type            Lavieville;
            alphaCrit       0.2;
        }
        nucleationSiteModel
        {
            type            LemmertChawla;
            Cn              185;
        }
        departureDiamModel
        {
            type            TolubinskiKostanchuk;
        }
        departureFreqModel
        {
            type            Cole;
        }
        LeidenfrostModel
        {
            type            Spiegler;
            Tcrit           647;
        }
        CHFModel
        {
            type            Zuber;
        }
        CHFSubCoolModel
        {
            type            HuaXu;
            Kburn           0.5;
        }
        MHFModel
        {
            type            Jeschar;
            Kmhf            1;
        }
        TDNBModel
        {
            type            Schroeder;
        }
        filmBoilingModel
        {
            type            Bromley;
            L               0.001;
        }
        value           uniform 1e-08;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 1e-08;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 1e-08;
    }
}


