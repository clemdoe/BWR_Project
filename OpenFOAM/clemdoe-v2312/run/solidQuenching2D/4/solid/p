FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "4/solid";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    symmetric
    {
        type            symmetry;
    }
    emptyPatch
    {
        type            empty;
    }
    solid_to_water
    {
        type            calculated;
        value           uniform 0;
    }
}


