//Mido los valores promedios en planos sucesivos
		Plano0
        {
                type            surfaceFieldValue;
                functionObjectLibs ("libfieldFunctionObjects.so");
				region          region0;
				writeControl     writeTime;        		
				writeFields     false;                
				regionType      sampledSurface;
                name			Plano0;
				sampledSurfaceDict
				{
					planeType       pointAndNormal;
					pointAndNormalDict
					{
							point   (0.00384 0 0);
							normal  (0 0 1);
						}
					type            plane;    
				}
				
				operation       areaAverage;
				fields
				(
				    alpha.steam alpha.steamMean T.waterMean T.water U.waterMean U.steamMean U.steam iDmdt.steamAndWaterMean wDmdt.steamAndWaterMean
				);
        }


