//Mido los valores promedios en PlanoAnulos sucesivos
		PlanoAnulo0
        {
                type            surfaceFieldValue;
                functionObjectLibs ("libfieldFunctionObjects.so");
				region          anulo;
				writeControl     writeTime;        		
				writeFields     false;                
				regionType      sampledSurface;
                name			PlanoAnulo0;
				sampledSurfaceDict
				{
					planeType       pointAndNormal;
					pointAndNormalDict
					{
							point   (0.00384 0 0);
							normal  (0 0 1);
						}
					type            plane;    
				}
				
				operation       average;
				fields
				(
				    T U
				);
        }


