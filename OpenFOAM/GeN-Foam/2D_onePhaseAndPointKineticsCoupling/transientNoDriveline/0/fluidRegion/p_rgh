FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{

    inlet
    {
        type            fixedFluxPressure;
        value 			$internalField;
    }
    outlet
    {
    	type            fixedValue;
    	value 			uniform 1e5;
    }
    fixedWalls
    {
    	type            fixedFluxPressure;
        value           $internalField;
    }
    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
