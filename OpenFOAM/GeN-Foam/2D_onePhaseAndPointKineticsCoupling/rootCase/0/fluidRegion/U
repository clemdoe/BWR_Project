FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform (0 0 1);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value 			uniform (0 0 1);
    }
    outlet
    {
    	type            zeroGradient;
    }
    fixedWalls
    {
    	type            slip;
    }
    frontAndBack
    {
        type            empty;
    }
}
