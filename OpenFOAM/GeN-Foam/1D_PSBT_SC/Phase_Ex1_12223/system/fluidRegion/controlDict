FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     FFSEulerFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;//

endTime         1;

deltaT          0.0001;

writeControl    adjustableRunTime;

writeInterval   0.02;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  true;

maxCo           2;

maxCoTwoPhase   0.5;

solveEnergy     true;

writeRestartFields true;

writeContinuityErrors true;

