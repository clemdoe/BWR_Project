FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0/fluidRegion";
    object      U.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (6.7 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           nonuniform List<vector> 0();
    }
    outlet
    {
        type            pressureInletOutletVelocity;
        phi             phi.liquid;
        value           uniform (6.7 0 0);
    }
    fixedWalls
    {
        type            slip;
    }
    frontAndright
    {
        type            symmetry;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform (6.7 0 0);
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform (6.7 0 0);
    }
}


