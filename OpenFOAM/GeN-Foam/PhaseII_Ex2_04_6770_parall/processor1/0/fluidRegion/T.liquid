FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/fluidRegion";
    object      T.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 587.8;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 587.8;
    }
    outlet
    {
        type            zeroGradient;
    }
    fixedWalls
    {
        type            zeroGradient;
    }
    frontAndright
    {
        type            symmetry;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 587.8;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 587.8;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 587.8;
    }
}


