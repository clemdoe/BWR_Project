FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/fuel";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 1000;

boundaryField
{
    bottom
    {
        type            zeroGradient;
        value           $internalField;
    }
    front
    {
        type            symmetry;
        value           $internalField;
    }
    fuel_to_cladding
    {
        type            compressible::turbulentTemperatureRadCoupledMixed;
        value           $internalField;
        Tnbr            T;
        kappaMethod     solidThermo;
    }
    top
    {
        type            zeroGradient;
        value           $internalField;
    }
}


